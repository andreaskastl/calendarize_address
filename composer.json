{
  "name": "andreaskastl/calendarize-address",
  "type": "typo3-cms-extension",
  "description": "Extends EXT:calendarize events with location and organizer records based on EXT:tt_address records and provides corresponding location and organizer views.",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": "^8",
    "typo3/cms-core": "^12.4 || ^13.4",
    "friendsoftypo3/tt-address": "^7 || ^8 || ^9",
    "lochmueller/calendarize": "^13"
  },
  "authors": [
    {
      "name": "Andreas Kastl",
      "role": "Developer",
      "email": "typo3@andreaskastl.de",
      "homepage": "https://github.com/andreaskastl/"
    }
  ],
  "keywords": [
    "typo3",
    "calendarize",
    "calendar",
    "event",
    "address"
  ],
  "support": {
    "issues": "https://github.com/andreaskastl/calendarize_address/issues"
  },
  "replace": {
    "typo3-ter/calendarize-address": "self.version"
  },
  "autoload": {
    "psr-4": {
      "AndreasKastl\\CalendarizeAddress\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "calendarize_address"
    }
  }
}
